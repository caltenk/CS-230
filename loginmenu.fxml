      <SplitPane dividerPositions="0.22690763052208834" orientation="VERTICAL" prefHeight="500.0" prefWidth="500.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" />
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="381.0" prefWidth="498.0">
                     <children>
                        <Button fx:id="newUserButton" alignment="CENTER" mnemonicParsing="false" onAction="#handleNewUserButton" text="New User">
                           <VBox.margin>
                              <Insets bottom="50.0" top="50.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#handleLoginButton" text="Login for previous user" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
